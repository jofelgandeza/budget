<section class="s1">
    <div class="main-container">
       <h4> BRANCHES OF OPERATIONS </h4>
        <table class="content-table">
            <tr>
                <% if (fondBranchs.length > 0) { %>
                <% } else { %>
                <% } %>

            </tr>
