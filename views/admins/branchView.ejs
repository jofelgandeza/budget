                    <td><%= areaBranch.branchDesc %> </td>
                    <td><%= areaBranch.branch_category %> </td>
                    <td>
                        <div class="btn-row">
                            <!-- <a class="btn btn-primary" href="/admins/getBranchForEdit/<%= areaCode + areaBranch.id %>/edit">Edit</a> -->
                            <!-- <%- include('../partials/deleteForm', { url: `/areas/deleteArea/${areaBranch.id}` }) %> -->
                        </div>        
                    </td>
                </tr>
            <% }) %> 
        </table>
        <div>
            <!-- <a class="btn btn-primary" href="/admins/newBranch/<%= areaCode %>">ADD NEW BRANCH</a> -->
            <a class="btn btn-primary" href="/admins/<%= yuser.assCode %>">DONE</a>
        </div>
    </div>
</section>

