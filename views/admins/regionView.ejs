<section class="s1">
    <div class="main-container">
       <h4> REGIONAL OPERATIONS</h4>
        <table class="content-table">
            <tr>
                <th> REGION CODE</th>
                <th> REGION DESCRIPTION</th>
            </tr>
            <% fondRegions.forEach(opRegion => { %>
                <tr>
                    <td><%= opRegion.regionCode %> </td>
                    <td><%= opRegion.regionDesc %> </td>
                    <td>
                        <div class="btn-row">
                        </div>        
                    </td>
                </tr>
            <% }) %> 
        </table>
        <div>
          <a class="btn btn-primary" href="/admins/<%= admin %>">DONE</a>
        </div>
    </div>
</section>

