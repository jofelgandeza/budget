	<section class="s1">
		<div class="main-container">

            <div class="intro-wrapper">
				<div class="nav-wrapper">

					<!-- Link around dots-wrapper added after tutorial video -->
					<a href="index.html">
						<div>  
						</div>
					</a>
					

					<ul id="navigation">
						<li><a href="index.html#contact">As of Date Today</Date></a></li>

					</ul>
				</div>

				<div class="left-column">
                    <table class="content-table-brnView">
                        <tr>
                            <th>BUDGET</th>
                            <th colspan="7"></th>
                        </tr>
                        <tr>
                            <th>LOAN TYPES</th>
                            <th>BEGINNING BALANCE</th>
                            <th>NEW CLIENTS</th>
                            <th>RESIGN CLIENTS</th>
                            <th>RE-LOAN CLIENTS</th>
                            <th>ENDING BALANCE</th>
                            <th></th>
                            <th>DISBURSEMENT AMOUNT</th>
                        </tr>
                        <% loanTots.forEach(loan => {  %>
                            <tr> 
                                <td><%= loan.loan_type %></td>
                                <% if (loan.begClientTot > 0) { %>
                                    <td><%= loan.begClientTot %></td>
                                    <% } else { %>
                                    <td>-</td>
                                <% } %>
                                <% if (loan.nnumClient > 0) { %>
                                    <td><%= loan.nnumClient %></td>
                                    <% } else { %>
                                    <td>-</td>
                                <% } %>
                                <% if (loan.resloanTot > 0) { %>
                                    <td><%= loan.resloanTot %></td>
                                    <% } else { %>
                                    <td>-</td>
                                <% } %>
                                <% if (loan.onumClient > 0) { %>
                                    <td><%= loan.onumClient %></td>
                                    <% } else { %>
                                    <td>-</td>
                                <% } %>
                                <% if (loan.budgEndBal > 0) { %>
                                    <td><%= loan.budgEndBal %></td>
                                    <% } else { %>
                                    <td>-</td>
                                <% } %>
                                <td></td>
                                <% if (loan.amtDisburse > 0) { %>
                                    <td><%= loan.amtDisburse.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ',') %></td>
                                    <% } else { %>
                                    <td>-</td>
                                <% } %>
                            </tr>
                        <% }) %>
                        <tr class="td-bold">
                            <td>TOTAL</td>
                            <% poGrandTot.forEach( gTot => { %>
                                <td> <%= gTot.bClient %></td>
                                <td> <%= gTot.nClient %></td>
                                <td> <%= gTot.rClient %></td>
                                <td> <%= gTot.oClient %></td>
                                <td> <%= gTot.budgEndBal %></td>
                                <td></td>
                                <td> <%= gTot.totDisburse.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ',') %></td>

                            <% }) %>
                        </tr>
                    </table>
				</div>

			</div>
		</div>
    </section>
    <div>
        <h5>
        </h5>
    </div>

