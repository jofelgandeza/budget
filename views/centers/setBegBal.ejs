
    <div class="box" id="heading">
        <h5> SET BEGINNING BALANCES FOR CENTER : <%= listTitle %> </h5>
      </div>
              <table>
                  <tr>
                      <th></th>
                      <th>LOAN TYPE</th>
                      <th>CLIENTS COUNT</th>
                      <th>PRINCIPAL AMT</th>
                      <th>INTEREST AMT</th>
                      <th>MATURITY</th>
                      <th></th>
                  </tr>
                  <% newListItems.forEach(function(item) { %>
                      <form  action="/centers/delBegBal" method="post">
                          <tr>
                              <td>
                                  <input type="checkbox" name="checkbox" value="<%=item._id%>" onChange="this.form.submit()">
                              </td>
                              <td>
                                  <p><%= item.loan_type %></p>
                              </td>
                              <td>
                                  <p><%= item.begBalClientCnt %></p>
                              </td>
                              <td>
                                <p><%= item.beg_principal %></p>                                  
                              </td>
                              <td>
                                  <p><%= item.beg_interest %></p>
                              </td>
                              <td>
                                <p><%= item.expected_maturity_date %></p>
                            </td>
                    </tr>
                      <input type="hidden" name="listName" value="<%= listTitle %>"> </input>
                  </form>
                  <% }) %>
                  <tr class="form-item-target">
                      <form action="/centers/putBegBal/<%= listTitle %>?_method=PUT" method="POST">
                        <%- include('../partials/errorMessage') %>    
                      <td>New:</td>
                      <td>
                          <select name="loanType" id="loanType">
                              <% loanType.forEach(function(loan_type, index) { %>
                                      <option label="<%= loan_type.title %>" value="<%= loan_type.title  %>"></option>
                              <% }) %>
                          </select>            
                      </td>
                      <td>
                          <input type="text" name="numClient" placeholder="No. of Clients" autocomplete="off">
                      </td>
                      <td>
                          <input type="text" class="begBalAmt" name="begBalAmt" placeholder="Principal Amt" autocomplete="off">
                      </td>
                      <td>
                        <input type="text" class="begBalInt" name="begBalInt" placeholder="Interest Amt" autocomplete="off">
                    </td>
                    <td>
                        <select name="numMonth" id="numMonth"">
                            <% monthSelect.forEach(function(month, index) { %> 
                                    <option label="<%= month %>" value="<%= month  %>"></option>
                            <% }) %>
                        </select>            
                    </td>
                    <td>
                          <button type="submit" name="list" value=<%= listTitle %>>+</button>
                    </td>
                      <td class="btn-row">
                              <a class="btn btn-primary" href="/centers/center/<%= unitID %>">Done</a>
  
                          </td>
                      </form>
                  </tr>
              </table>
  