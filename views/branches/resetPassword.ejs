<h2 class="page-header">Edit Employee -> Reset Password</h2>
<form action="/branches/putEditedPass/<%= emp.branch + emp._id %>?_method=PUT" method="POST">
    <%- include('_form_fields') %>
    <div class="form-item">
        <div class="form-row">
            <button class="btn btn-primary" type="submit">Reset</button>
            <a class="btn btn-primary" href="/branches/employees/<%= emp.branch %>">Cancel</a>
        </div>
    </div>
</form>