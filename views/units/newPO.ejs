<h2 class="page-header">New PO Entry - <%= unitCode %> </h2>
<form action="/units/postNewPO/<%= unitCode %>" method="POST">
            <%- include('../partials/errorMessage') %>    
            <%- include('_form_fields') %>
<div class="form-row">
    <div class="form-item-column"></div>
    <div class="form-item-column">
        <a class="btn btn-primary" href="/units/<%= unitCode %>">Cancel</a>
    </div>
    <div class="form-item-column">
        <button class="btn btn-primary" type="submit">Create</button>
    </div>
    <div class="form-item-column"></div>
</div>
</form>