<section class="s1">
    <div class="main-container">
       <h4> REGIONAL DIRECTORS MAINTENANCE</h4>
        <table class="content-table">
            <tr>
                <th> NAME</th>
                <th> REGION ASSIGNMENT</th>
                <th> LINKS</th>
            </tr>
            <% fndEmploy.forEach(brnEmployee => { %>
                <tr>
                    <td><%= brnEmployee.empName %> </td>
                    <td><%= brnEmployee.empPost %> </td>
                    <td>
                        <div class="btn-row">
                            <a class="btn btn-primary" href="/deds/getEmpForEdit/<%= ded + brnEmployee.empCode %>/edit">Edit</a>
                            <a class="btn btn-primary" href="/deds/getEmpEditPass/<%= ded + brnEmployee.empCode %>/edit">Reset Password</a>
                            <!-- <%- include('../partials/deleteForm', { url: `/deds/deleteEmp/${brnEmployee.empID}` }) %> -->
                        </div>        
                    </td>
                </tr>
            <% }) %> 
        </table>
        <div>
            <a class="btn btn-primary" href="/deds/newEmployee/<%= ded %>">ADD NEW REGIONAL DIRECTOR</a>
            <a class="btn btn-primary" href="/deds/<%= ded %>"">DONE</a>
        </div>
    </div>
</section>

