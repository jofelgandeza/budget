<h2 class="page-header">New Unit Entry - <%= branchCode %></h2>
<form action="/branches/postNewUnit/<%= branchCode %>" method="POST">
            <%- include('../partials/errorMessage') %>    
            <%- include('_unit_form_fields') %>
<div class="form-row">
    <div class="form-item-column"></div>
    <div class="form-item-column">
        <a class="btn btn-primary" href="/branches/units/<%= branchCode %>">Cancel</a>
    </div>
    <div class="form-item-column">
        <button class="btn btn-primary" type="submit">Create</button>
    </div>
    <div class="form-item-column"></div>
</div>
</form>